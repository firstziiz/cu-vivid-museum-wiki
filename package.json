{
    "name": "vivid-museum",
    "version": "1.0.0",
    "main": "index.js",
    "repository": "https://github.com/zapkub/vivid-museum",
    "author": "Rungsikorn Rungsikavanich <rungsikorn@me.com>",
    "license": "MIT",
    "scripts": {
        "start": "node ./entry.js",
        "dev": "sh ./.bin/dev.sh",
        "watch": "nodemon --debug -w ./entry -w ./server ./entry.js",
        "build": "next build",
        "json": "node ./seed/convertToJson.js",
        "clean": "sh .bin/clean.sh",
        "down": "forever stopall",
        "up": "forever start ./entry.js",
        "prod": "sh .bin/prod.sh",
        "alpha": "sh .bin/alpha.sh",
        "test": "jest || true",
        "commit": "git-cz || true"
    },
    "dependencies": {
        "@types/graphql": "^0.8.6",
        "apollo-client": "^0.10.1",
        "babel-preset-stage-0": "^6.22.0",
        "chai": "^3.5.0",
        "chalk": "^1.1.3",
        "cloudinary-core": "^2.1.9",
        "connect-mongo": "^1.3.2",
        "cors": "^2.8.1",
        "dotenv": "^4.0.0",
        "forever": "^0.15.3",
        "graphql": "^0.9.1",
        "graphql-anywhere": "^2.2.0",
        "graphql-compose": "^1.14.0",
        "graphql-compose-connection": "^2.1.3",
        "graphql-compose-mongoose": "^1.4.8",
        "graphql-server-express": "^0.6.0",
        "graphql-tag": "^1.3.1",
        "graphql-tools": "^0.10.1",
        "jest": "^19.0.2",
        "keystone": "^4.0.0-beta.5",
        "lodash": "^4.17.4",
        "mongoose": "^4.8.5",
        "mongoose-text-search": "^0.0.2",
        "next": "2.0.0-beta.39",
        "node-xlsx": "^0.7.4",
        "query-string": "^4.3.2",
        "react": "^15.4.2",
        "react-apollo": "latest",
        "react-dom": "^15.4.2",
        "react-highlight-words": "^0.6.0",
        "react-image-gallery": "^0.7.13",
        "react-paginate": "^4.2.0",
        "recompose": "^0.22.0",
        "redux-logger": "^2.8.2",
        "redux-thunk": "^2.2.0",
        "semantic-ui-css": "^2.2.9",
        "semantic-ui-react": "^0.67.0"
    },
    "devDependencies": {
        "babel-eslint": "^7.1.1",
        "commitizen": "^2.9.6",
        "cz-conventional-changelog": "^2.0.0",
        "eslint": "latest",
        "eslint-config-airbnb": "^14.1.0",
        "eslint-config-clogii": "^1.0.0",
        "eslint-plugin-flow-vars": "^0.5.0",
        "eslint-plugin-flowtype": "2.30.0",
        "eslint-plugin-import": "^2.2.0",
        "eslint-plugin-jsx-a11y": "^4.0.0",
        "eslint-plugin-react": "^6.10.0",
        "nodemon": "^1.11.0"
    },
    "jest": {
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ]
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    }
}
